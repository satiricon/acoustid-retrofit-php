{
  "name": "satiricon/retro-acoustid",
  "description": "Acoustic ID API implementation with retrofit-php",
  "type": "library",
  "keywords": [
    "acoustid",
    "library",
    "php",
    "api"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "GNU AGPL",
  "authors": [
    {
      "name": "Ruben David Gomez",
      "email": "david.satiricon@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "tebru/retrofit-php": "~3.0",
    "tebru/retrofit-php-http-guzzle6" : "^0.1",
    "netresearch/jsonmapper": "^1.6"
  },
  "require-dev": {
    "symfony/dependency-injection": "^5.0",
    "symfony/var-dumper": "*",
    "vlucas/phpdotenv": "^3.4",
    "phpunit/phpunit": "^8"
  },
  "autoload": {
    "psr-4": {
      "Satiricon\\AcoustId\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Satiricon\\AcoustId\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "component-dir": "public/packages",
    "vendor-dir": "vendor"
  }
}
